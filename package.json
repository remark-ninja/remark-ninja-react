{
  "name": "remark-ninja-react",
  "version": "2.4.2",
  "description": "React component for Remark Ninja",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "lib/*"
  ],
  "scripts": {
    "build": "nwb build-react-component && cp src/*.css lib/",
    "clean": "nwb clean-module && nwb clean-demo",
    "prepublishOnly": "npm run build",
    "start": "nwb serve-react-demo",
    "test": "nwb test-react",
    "test:coverage": "nwb test-react --coverage",
    "test:watch": "nwb test-react --server",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "axios": "^0.21.1"
  },
  "peerDependencies": {
    "react": "16.x",
    "react-dom": "16.x"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-links": "^6.0.21",
    "@storybook/addons": "^6.0.21",
    "@storybook/react": "^6.0.21",
    "babel-loader": "^8.1.0",
    "nwb": "^0.25.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "author": "Remark Ninja <support@remark.ninja>",
  "homepage": "https://remark.ninja",
  "license": "MIT",
  "repository": "github:remark-ninja/remark-ninja-react",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "comments",
    "react-component"
  ]
}
